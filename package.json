{
  "name": "random-number",
  "repository": "https://github.com/SW999/random-number",
  "homepage": "https://sw999.github.io/random-number",
  "main": "src/index.tsx",
  "jest": {
    "moduleNameMapper": {
      "^.+\\.svg$": "<rootDir>/svgTransform.js"
    },
    "verbose": false,
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ]
  },
  "version": "0.1.7",
  "description": "",
  "scripts": {
    "clean": "del-cli dist/*",
    "start": "cross-env NODE_ENV=development && npm run clean && parcel index.html --open",
    "build": "cross-env NODE_ENV=production && npm run clean && parcel build index.html --public-url ./ --no-cache --no-source-maps",
    "prebuild": "npm version patch",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "jest --watch",
    "lint": "eslint src/ --ext .js --ext .jsx"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "babel-eslint": "^10.0.3",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.10",
    "del-cli": "^1.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^6.2.2",
    "eslint-plugin-react": "^7.14.3",
    "gh-pages": "^2.0.1",
    "jest": "^24.7.1",
    "sass": "^1.22.7",
    "typescript": "^3.5.2"
  }
}
